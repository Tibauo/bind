$TTL 86400
@   IN  SOA     bind.{{ domain_name }}. root.{{ domain_name }}. (
                                              3           ;Serial
                                              3600        ;Refresh
                                              1800        ;Retry
                                              604800      ;Expire
                                              86400       ;Minimum TTL
)

;Name Server Information
@  IN  NS bind.mylab.local.
{% for def in forwardzone[0].def %}
{{ def.name }}    IN      {{ def.record }}       {{ def.ip }}
{% endfor %}
